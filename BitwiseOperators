import java.util.Scanner;
class BitwiseOPerators
{
  public static void main(String[] args)
  {
      int x,y;
      Scanner sc=new Scanner(System.in);
      x=sc.nextInt();
      y=sc.nextInt();
      int z=x&y;      //using and(&) operator if both bits are are 1 it returns 1 others wise it returns 0
      System.out.println("BITWISE-AND of "+x+" and "+y+" is " +z);
      z=x|y;          //using or(|) operator it returns 1 when any one of the bit is one other wise it returns 0
      System.out.println("BITWISE-AND of "+x+" and "+y+" is " +z);
      z=x^y;          //using xor(^) operator it returns 0 when both bits are same other wise it returns 1
      System.out.println("BITWISE-AND of "+x+" and "+y+" is " +z);
   }
}
